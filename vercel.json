{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/merge",
      "dest": "server.js"
    },
    {
      "src": "/(CSS|JS|favicon.ico|merged_files)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
